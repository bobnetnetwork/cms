{{> backoffice/header}}

<div class="card shadow mb-4">
    <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary"><a href="{{home}}/admin/articles/new">{{lang.addNew}}</a></h6>
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                <thead>
                <tr>
                    <th>ID</th>
                    <th>{{lang.name}}</th>
                    <th>{{lang.registeredAt}}</th>
                </tr>
                </thead>
                <tfoot>
                <tr>
                    <th>ID</th>
                    <th>{{lang.name}}</th>
                    <th>{{lang.registeredAt}}</th>
                </tr>
                </tfoot>
                <tbody>
                {{#users}}
                    <tr>
                        <td></td>
                        <td><a href="{{home}}/admin/users/{{userName}}">{{firstName}} {{lastName}}</a></td>
                        <td>{{registeredAt}}</td>
                    </tr>
                {{/users}}
                </tbody>
            </table>
        </div>
    </div>
</div>




{{> backoffice/footer}}